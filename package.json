{
  "name": "invest-workbench",
  "productName": "invest-workbench",
  "version": "0.1.0-alpha",
  "description": "Models that map and value the goods and services from nature that sustain and fulfill human life",
  "main": "src/main.js",
  "homepage": "./",
  "scripts": {
    "start": "cross-env FLASK_APP=src/server.py npx electron . --dev",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "test": "cross-env DEBUG_PRINT_LIMIT=10 jest --testMatch **/tests/*.test.js",
    "test-flask-app": "jest --runInBand --testMatch **/tests/binary_tests/flaskapp.test.js",
    "test-electron-app": "jest --runInBand --testMatch **/tests/binary_tests/puppet.test.js",
    "postinstall": "electron-builder install-app-deps",
    "build": "npx ./build.js",
    "dist": "electron-builder -c.extraMetadata.main=build/main.js",
    "pack": "electron-builder --dir -c.extraMetadata.main=build/main.js"
  },
  "build": {
    "extraResources": [
      {
        "from": "build/invest",
        "to": "invest"
      }
    ],
    "appId": "NaturalCapitalProject.Invest.Workbench",
    "productName": "InVEST Workbench",
    "artifactName": "invest-workbench_${os}_${arch}_${version}.${ext}",
    "mac": {
      "category": "public.app-category.business",
      "icon": "build/icon.png",
      "target": "zip"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "build/icon.png",
      "category": "Science"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "testEnvironment": "jest-environment-jsdom-sixteen"
  },
  "invest": {
    "version": "3.8.9"
  },
  "keywords": [],
  "author": "dmf",
  "license": "MIT",
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "chroma-js": "^2.1.0",
    "cross-env": "^5.2.1",
    "esm": "^3.2.25",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.6",
    "google-palette": "^1.1.0",
    "leaflet": "^1.7.1",
    "node-fetch": "^2.6.1",
    "papaparse": "^5.3.0",
    "prop-types": "^15.7.2",
    "radium": "^0.25.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-easy-chart": "^1.0.0",
    "react-hot-loader": "^3.1.3",
    "react-lazylog": "^4.5.3",
    "react-leaflet": "^2.7.0",
    "react-leaflet-choropleth": "^2.0.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "sanitize-html": "^2.1.0",
    "tail": "^2.0.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "@jest-runner/electron": "^2.0.3",
    "@testing-library/dom": "^7.24.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^29.2.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "fs-extra": "^9.0.1",
    "jest": "^26.4.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "pptr-testing-library": "^0.6.4",
    "puppeteer-core": "^5.3.0",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
